{
  "name": "webapp-v2",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview --host",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check \"src/**/(*.tsx|*.ts|*.css)\"",
    "prettier:fix": "prettier --write \"src/**/(*.tsx|*.ts|*.css)\"",
    "format": "prettier --write \"src/**/(*.tsx|*.ts|*.css)\" && eslint . --fix",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@ant-design/charts": "^2.2.6",
    "@ant-design/icons": "^5.6.0",
    "@ant-design/plots": "^2.3.3",
    "@tanstack/react-query": "^5.66.0",
    "@tanstack/react-query-devtools": "^5.66.0",
    "antd": "^5.14.0",
    "axios": "^1.7.9",
    "clsx": "^2.1.0",
    "date-fns": "^4.1.0",
    "react": "^18.2.0",
    "react-apexcharts": "^1.7.0",
    "react-component-export-image": "^1.0.6",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@tanstack/eslint-plugin-query": "^5.66.0",
    "@types/node": "^20.11.16",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.22.0",
    "vite": "^5.0.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier:fix && npm run lint:fix"
    }
  }
}
